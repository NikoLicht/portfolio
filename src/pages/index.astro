---
import MainLayout from "../layouts/MainLayout.astro";
import * as info from '../pages/info/get_to_know_me.md';
import ProjectCard from "../layouts/ProjectCard.astro";
let allProjects = await Astro.glob("../pages/projects/*.md");
import GradientBox from "../layouts/GradientBox.astro";




const heroImg = (await import('../images/info/full_figure_extended.png')).default;
---
<MainLayout title="Nikolaj Licht // Creative Dev">
    <div>
        <GradientBox showNavigation={true}>
            <div class="top-section-content .background">
                <div class="me_heading upper-left">
                    <h1>{info.frontmatter.job_title}</h1>
                </div>

                <div class="me_heading" id="me_image_container">
                    <img id="me_image" src={heroImg} width="2000" alt="Photograph of Nikolaj Licht, very employable looking"/>
                </div>

                <div class="me_heading lower-right">
                    <h1>{info.frontmatter.name_first}</h1>
                    <h1>{info.frontmatter.name_last}</h1>
                </div>
            </div>
        </GradientBox>
        <GradientBox showNavigation={false}>
            <div class="bottom-section-content">
                <h2 class="me_heading projects-title">Projects and Work//</h2>
                <div class="projects">
                {
                    allProjects.sort((a, b) =>{
                        return a.frontmatter.order - b.frontmatter.order;
                    }).map((post) => <ProjectCard {post} />)
                }
                </div>
            </div>
        </GradientBox>
    </div>
</MainLayout>
<style>

    #me_image_container{
        grid-area: picture;
        max-height: 100%;
        max-width: 100%;
        overflow: hidden;
        display: flex;
        justify-content: space-around;
        align-items: flex-end;
        background-color: white;
    }

    #me_image{
        max-height: 100%;
        min-width: 100%;
        max-width: 100%;
        display: block; 
        object-fit: cover;
    }

    .projects{
        display: flex;
        flex-wrap:wrap;
        justify-content:center;
        max-width:100%;
        padding: 1em;
        gap: 1em;
    }

    .projects-title{
        padding-left:0.5em;
    }

    .top-section-content{
        display: grid;
        max-height: 80vh;
        grid-template-columns: 5fr minmax(4em, 20%) 5fr ;
        grid-template-rows: 1fr 3fr 1fr;
        grid-template-areas: 
        " upperLeft . upperRight "
        " picture picture picture "
        " . . lowerRight ";
        gap: 1em;
    }

    .background {
        position: absolute;
        width: 100vw;
        height: 100vh;
        overflow: hidden;
        background: radial-gradient(ellipse at center, #ffeeac 0%, #5b2c15 100%);
        background-image: url(data:image/png;base64,);
    }

    .background::after {
        content:"";
        position: absolute;
        width:200vw;
        height: 200vh;
        left: -50%;
        top: -50%;
        background-image: url(data:image/png;base64,);
        animation:grain 1.5s steps(6) infinite;
    }

    .grain-img {
        z-index: -1;
    }

    @keyframes grain {
        0%, 100% {transform: translate(0,0);}
        10% {transform: translate(-5%,-10%);}
        30% {transform: translate(3%,-15%);}
        50% {transform: translate(12%,9%);}
        70% {transform: translate(9%,4%);}
        90% {transform: translate(-1%,7%);}
    }

    .me_heading{
        font-size: 2vi;
        color: var(--dark-color);
        text-transform: uppercase;
        vertical-align: middle;
    }
    .me_heading > h1{
        margin-top: 0;
        margin-bottom: 0;
    }

    .upper-left{
        grid-area: upperLeft;
    }

    .upper-right{
        grid-area: upperRight;
    }

    .lower-right{
        grid-area: lowerRight;
        text-align: right;
        display: flex;
        justify-content: flex-end;
        align-items: flex-end;
        align-content: flex-end;
        flex-wrap: wrap;
    }

    .lower-right > h1{
        width: 100%;
        bottom:0;
        flex: 0 0 5em;;
    }


    @media only screen and (min-width: 1200px) and (min-aspect-ratio: 1440/1050){
        #me_image{
            object-position: 0 -11vw;
            color: red;
        }
    }


    @media only screen and (max-width: 600px) and (orientation: portrait) {
        .projects{
            padding: 2em;
            gap: 0.5em;
        }

        .projects-title{
            text-align: center;
            font-size: 2em;
        }

        .me_heading{
            font-size: 5vi;
            max-width: 100%;
        }

        .top-section-content{
            max-width: 100%;
            width: 100%;
        }

        .top-section-content{
            grid-template-areas: 
            " upperLeft upperLeft upperRight "
            " picture picture picture "
            " lowerRight lowerRight lowerRight ";
        }

        #me_image_container{
            max-height: unset;
            max-width: unset;
            overflow: unset;
            display: unset;
            justify-content: unset;
            align-items: unset;
            background-color: unset;
        }

        #me_image{
            display: block;
            max-width: 100%;
            min-width: unset;
            min-height: 100%;
            max-height: 100%;
            float: left;
            height: 100%;
            object-fit: cover;
        }

    }
</style>